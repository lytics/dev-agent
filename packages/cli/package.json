{
  "name": "@lytics/dev-agent-cli",
  "version": "0.1.2",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "dev": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js",
      "require": "./dist/cli.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.js",
    "!dist/**/*.test.d.ts",
    "!dist/**/*.test.d.ts.map",
    "!dist/**/*.test.js.map",
    "!dist/**/__tests__"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@lytics/dev-agent-core": "workspace:*",
    "@lytics/dev-agent-mcp": "workspace:*",
    "@lytics/dev-agent-subagents": "workspace:*",
    "@lytics/kero": "workspace:*",
    "chalk": "^5.6.2",
    "commander": "^12.1.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.3.3"
  }
}
