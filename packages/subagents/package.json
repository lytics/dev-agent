{
  "name": "@lytics/dev-agent-subagents",
  "version": "0.5.2",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.js",
    "!dist/**/*.test.d.ts",
    "!dist/**/*.test.d.ts.map",
    "!dist/**/*.test.js.map",
    "!dist/**/__tests__"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@lytics/dev-agent-core": "workspace:*",
    "@lytics/dev-agent-types": "workspace:*",
    "@lytics/kero": "workspace:*",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.3.3"
  }
}
