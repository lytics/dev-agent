{
  "name": "@lytics/dev-agent-types",
  "version": "0.2.0",
  "description": "Shared TypeScript types for dev-agent packages",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./github": {
      "types": "./dist/github.d.ts",
      "import": "./dist/github.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@lytics/kero": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "private": true
}
