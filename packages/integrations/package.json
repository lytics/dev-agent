{
  "name": "@lytics/dev-agent-integrations",
  "version": "0.1.20",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@lytics/dev-agent-core": "workspace:*",
    "@lytics/dev-agent-cli": "workspace:*",
    "@lytics/dev-agent-subagents": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
